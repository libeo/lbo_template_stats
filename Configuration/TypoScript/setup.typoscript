        layoutRootPaths.1 = {$module.tx_lbotemplatestats_templatestats.view.layoutRootPath}
    }
    settings {
        cacheName = {$module.tx_lbotemplatestats_templatestats.settings.lbo_template_stats_cacheName}
    }
}
